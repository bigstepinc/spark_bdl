<configuration>
    <property>
        <name>fs.bdl.impl</name>
        <value>com.bigstep.bdl.BDLFileSystem</value>
    </property>

    <property>
        <name>fs.bdl.auth.apiKey</name>
        <value>AUTH_APIKEY</value>
    </property>
    
    <property>
        <name>fs.bdl.api.endpoint</name>
